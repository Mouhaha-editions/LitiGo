{% trans_default_domain 'FOSUserBundle' %}

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}
<div class="auth">
    <div class="auth-container">
        <div class="card">
            <header class="auth-header">
                <h1 class="auth-title">
                    <div class="logo">
                        <span class="l l1"></span>
                        <span class="l l2"></span>
                        <span class="l l3"></span>
                        <span class="l l4"></span>
                        <span class="l l5"></span>
                    </div> Litigo </h1>
            </header>
            <div class="auth-content">
                <form id="login-form" action="{{ path("fos_user_security_check") }}" method="POST" novalidate="novalidate">
                    {% if csrf_token %}
                <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                    {% endif %}
                    <div class="form-group">
                        <label for="username">Identifiant ou email</label>
                        <input type="text" class="form-control underlined" id="username" name="_username" value="{{ last_username }}" required="required" placeholder="{{ 'security.login.username'|trans }}" autocomplete="username" data-kwimpalastatus="alive" data-kwimpalaid="1545073538944-2"> <span style="opacity: 1; left: 378px; top: 230px; width: 19px; min-width: 19px; height: 13px; position: absolute; background-image: url(&quot;data:image/svg+xml;base64,&quot;); background-repeat: no-repeat; background-position: 0px 0px; border: none; display: inline; visibility: visible; z-index: auto;"></span></div>
                    <div class="form-group">
                        <label for="password">Mot de passe</label>
                        <input type="password" class="form-control underlined" id="password" name="_password" required="required"  placeholder="{{ 'security.login.password'|trans }}" autocomplete="current-password"  data-kwimpalastatus="alive" data-kwimpalaid="1545073538944-3"> <span style="opacity: 1; left: 378px; top: 315px; width: 19px; min-width: 19px; height: 13px; position: absolute; background-image: url(&quot;data:image/svg+xml;base64,&quot;); background-repeat: no-repeat; background-position: 0px 0px; border: none; display: inline; visibility: visible; z-index: auto;"></span></div>
                    <div class="form-group">
                        <label for="remember" data-children-count="1">
                            <input type="checkbox" id="chb1" name="_remember_me" value="on" >
                            <label for="chb1">{{ 'security.login.remember_me'|trans }}</label>
                        </label>
                        {#<a href="reset.html" class="forgot-btn pull-right">Forgot password?</a>#}
                    </div>
                    <div class="form-group">
                        <button type="submit" name="_submit" class="btn btn-block btn-primary">{{ 'security.login.submit'|trans }}</button>
                    </div>
                    {#<div class="form-group">#}
                        {#<p class="text-muted text-center">Do not have an account?#}
                            {#<a href="signup.html">Sign Up!</a>#}
                        {#</p>#}
                    {#</div>#}
                </form>
            </div>
        </div>

    </div>
</div>